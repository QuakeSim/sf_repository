     </init-param>
      <init-param>
      <name>HelpPage</name>
      <value>/DislocHelp.jsp</value>
     </init-param>
     <supports>
       <mime-type>text/html</mime-type>
       <portlet-mode>VIEW</portlet-mode>
       <portlet-mode>HELP</portlet-mode>
     </supports>
     <portlet-info>
      <title>Disloc2</title>
      <keywords>Disloc</keywords>
     </portlet-info>        
   </portlet>

