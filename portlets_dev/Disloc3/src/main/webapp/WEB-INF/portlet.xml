	<portlet>
    <portlet-name>pick-location-portlet</portlet-name>
    <portlet-class>org.apache.portals.bridges.jsf.FacesPortlet</portlet-class>
      <expiration-cache>0</expiration-cache>
      <init-param>
      <!--
      <name>default-view</name>
       -->
      <name>ViewPage</name>
      <value>/LocateProject.jsp</value>
     </init-param>
      <init-param>
      <name>HelpPage</name>
      <value>/DislocHelp.jsp</value>
     </init-param>
     <supports>
