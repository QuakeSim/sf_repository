<portlet-tabbed-pane>
    <portlet-tab label="Disloc2">
        <title lang="en">Disloc2</title>
        <portlet-tabbed-pane style="sub-menu">
            <portlet-tab label="Disloc2-Setup">
                <title lang="en">Run Disloc</title>
                <table-layout>
                    <row-layout>
                        <column-layout>
                            <portlet-frame label="Disloc2-PF">
                                <portlet-class>Disloc2#Disloc2</portlet-class>
                            </portlet-frame>
                        </column-layout>
                    </row-layout>
                </table-layout>
            </portlet-tab>
              <portlet-tab label="Fetch-Disloc2-results">
                <title lang="en">Fetch Disloc Results</title>
                <table-layout>
                    <row-layout>
                        <column-layout>
                            <portlet-frame label="Disloc2-fetchmesh-PF">
                                <portlet-class>Disloc2#fetchresults-portlet</portlet-class>
                            </portlet-frame>
                        </column-layout>
                    </row-layout>
                </table-layout>
            </portlet-tab>
              <portlet-tab label="Plot-disloc2-results">
                <title lang="en">Plot Disloc Results</title>
                <table-layout>
                    <row-layout>
                        <column-layout>
                            <portlet-frame label="Disloc2-plot-grid-PF">
                                <portlet-class>Disloc2#plot-results-portlet</portlet-class>
                            </portlet-frame>
                        </column-layout>
                    </row-layout>
                </table-layout>
            </portlet-tab>
              <portlet-tab label="Pick-Location">
                <title lang="en">Pick Location</title>
                <table-layout>
                    <row-layout>
                        <column-layout>
                            <portlet-frame label="Disloc2-pick-location-PF">
                                <portlet-class>Disloc2#pick-location-portlet</portlet-class>
                            </portlet-frame>
                        </column-layout>
                    </row-layout>
                </table-layout>
            </portlet-tab>
        </portlet-tabbed-pane>
    </portlet-tab>
</portlet-tabbed-pane>



