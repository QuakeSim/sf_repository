<?xml version="1.0" encoding="UTF-8" ?> 
<Module>
  <ModulePrefs title="Disloc2" width="700" height="700" scrolling="true"> 
    <Require feature="tabs" /> 
  </ModulePrefs>
  <Content type="html">
  <![CDATA[ 

    <script type="text/javascript">

    var tabs = new gadgets.TabSet(__MODULE_ID__, "LoadProject");
    function init() {

        tabs.addTab("LoadProject", {
           contentContainer: document.getElementById("LoadProject_id")
        });

        tabs.addTab("FetchResults", {
           contentContainer: document.getElementById("FetchResults_id")
        });

        tabs.addTab("LocateProject", {
           contentContainer: document.getElementById("LocateProject_id")
        });


    }

    // Call init function to initialize and display tabs.
    gadgets.util.registerOnLoadHandler(init);

   </script>


   <div id="LoadProject_id" style="display:none">
       <iframe scrolling="yes" width=100% height="600" frameborder="0" src="http://156.56.104.158:8080//Disloc2/LoadProject.faces"></iframe>
   </div>

   <div id="FetchResults_id" style="display:none">
       <iframe scrolling="yes" width=100% height="600" frameborder="0" src="http://156.56.104.158:8080//Disloc2/FetchResults.faces"></iframe>
   </div>

   <div id="LocateProject_id" style="display:none">
       <iframe scrolling="yes" width=100% height="600" frameborder="0" src="http://156.56.104.158:8080//Disloc2/LocateProject.faces"></iframe>
   </div>

  ]]>
  </Content>
</Module>
