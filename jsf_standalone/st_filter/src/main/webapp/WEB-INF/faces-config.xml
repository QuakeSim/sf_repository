<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd" >

<faces-config>
	
	<!-- managed beans of the simple hello world app -->
	<managed-bean>
		<managed-bean-name>stfilterBean</managed-bean-name>
		<managed-bean-class>org.apache.myfaces.blank.STFILTERBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
                <!--Who we are-->
                <managed-property>
                   <property-name>codeName</property-name>
                   <value>STFILTER</value>
                </managed-property>

                <!-- Needed for the context service-->
                <managed-property>
                   <property-name>contextUrl</property-name>
                   <value>http://gf2.ucs.indiana.edu:8080/WebServices/services/ContextManager</value>
                 </managed-property>
                 <managed-property>
                   <property-name>contextBasePath</property-name>
                   <value>/home/gateway/QuakeSim2/portal_deploy/apache-tomcat-5.5.12/webapps/WebServices/WEB-INF/Descriptors/users/</value>
                </managed-property>

                <!-- These are execution host params -->
                <managed-property>
                   <property-name>antUrl</property-name>
                   <value>
                     http://danube.ucs.indiana.edu:8045/axis/services/AntVisco
                   </value>
                </managed-property>
                <managed-property>
                   <property-name>binPath</property-name>
                   <value>/home/gateway/GEMCodes/STFILTER2/bin/</value>
                </managed-property>
                <managed-property>
                   <property-name>baseWorkDir</property-name>
                   <value>/home/gateway/STFILTER_WDIR</value>
                </managed-property>
                <managed-property>
                   <property-name>fileServiceUrl</property-name>
                   <value>http://danube.ucs.indiana.edu:8045/GCWS/services/FileService</value>
                </managed-property>


                <!-- These are gnuplot host params -->
                <managed-property>
                   <property-name>gnuplotAntUrl</property-name>
                   <value>
                     http://danube.ucs.indiana.edu:8045/axis/services/AntVisco
                   </value>
                 </managed-property>
                <managed-property>
                   <property-name>gnuplotBinPath</property-name>
                   <value>/home/gateway/GEMCodes/Gnuplot/stfilter/plotXYZ/</value>
                 </managed-property>
                <managed-property>
                   <property-name>gnuplotBaseWorkDir</property-name>
                   <value>/home/gateway/STFILTER_WDIR</value>
                 </managed-property>
                <managed-property>
                   <property-name>gnuplotFileServiceUrl</property-name>
                   <value>http://danube.ucs.indiana.edu:8045/GCWS/services/FileService</value>
                 </managed-property>

	</managed-bean>
	
	<!-- navigation rules for Main.jsp -->
	<navigation-rule>
		<from-view-id>/Main.jsp</from-view-id>
		<navigation-case>
			<from-outcome>new-project-created</from-outcome>
			<to-view-id>/NewProject.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>list-old-projects</from-outcome>
			<to-view-id>/LoadProject.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>load-database-page</from-outcome>
			<to-view-id>/DBQuery.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>load-data-archive</from-outcome>
			<to-view-id>/DataArchive.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>list-project-plots</from-outcome>
			<to-view-id>/PlotData.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- navigation rules for NewProject.jsp -->
	<navigation-rule>
		<from-view-id>/NewProject.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>parameters-set</from-outcome>
			<to-view-id>/SetInputFile.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>parameters-to-textfield</from-outcome>
			<to-view-id>/SetInputFile.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>stfilter-parameters-to-database</from-outcome>
			<to-view-id>/SetInputFileFromDB.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>parameters-to-googlemap</from-outcome>
			<to-view-id>/allstations.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

       
        
        <!-- Allstations navigation rules -->
	<navigation-rule>
		<from-view-id>/allstations.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>parameters-to-database</from-outcome>
			<to-view-id>/SetInputFileFromDB.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
        
        <!-- SetInputFile navigation rules -->
	<navigation-rule>
		<from-view-id>/SetInputFile.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>stfilter-launched</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

        <!-- SetInputFileFromDB navigation rules -->
	<navigation-rule>
		<from-view-id>/SetInputFileFromDB.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>stfilter-display-query-results</from-outcome>
			<to-view-id>/SetInputFile.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
        
        <!-- LoadProject navigation rules -->
	<navigation-rule>
		<from-view-id>/LoadProject.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/LoadProject.jsp</from-view-id>
		<navigation-case>
			<from-outcome>project-populated</from-outcome>
			<to-view-id>/NewProject.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
        
        <!-- DB query rules -->
	<navigation-rule>
		<from-view-id>/DBQuery.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back-to-main</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>stfilter-display-query-results</from-outcome>
			<to-view-id>/DisplayQueryResults.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

        <!-- Display query rules -->
	<navigation-rule>
		<from-view-id>/DisplayQueryResults.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>project-populated</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

        <!-- Data archive rules -->
	<navigation-rule>
		<from-view-id>/DataArchive.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back-to-main</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>download-data-file</from-outcome>
			<to-view-id>/DownloadData.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

        <!-- Plot data rules -->
	<navigation-rule>
		<from-view-id>/PlotData.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>plot-created</from-outcome>
			<to-view-id>/ViewPlot.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
        <!-- ViewPlot rules -->
	<navigation-rule>
		<from-view-id>/ViewPlot.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>