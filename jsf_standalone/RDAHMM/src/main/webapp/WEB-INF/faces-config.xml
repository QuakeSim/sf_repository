<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd" >

<faces-config>
	
	<!-- managed beans of the simple hello world app -->
	<managed-bean>
		<managed-bean-name>rdahmmBean</managed-bean-name>
		<managed-bean-class>org.apache.myfaces.blank.RDAHMMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- navigation rules for helloWorld.jsp -->
	<navigation-rule>
		<from-view-id>/Main.jsp</from-view-id>
		<navigation-case>
			<from-outcome>create-new-project</from-outcome>
			<to-view-id>/NewProject.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/Main.jsp</from-view-id>
		<navigation-case>
			<from-outcome>list-old-projects</from-outcome>
			<to-view-id>/LoadProject.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- navigation rules for page2.jsp -->
	<navigation-rule>
		<from-view-id>/NewProject.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/LoadProject.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/page2.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Main.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>