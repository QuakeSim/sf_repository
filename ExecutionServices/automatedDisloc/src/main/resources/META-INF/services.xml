<serviceGroup>
  <service name="AutomatedDislocBean">
	 <Description>
		Runs Disloc automatically using USGS earthquake RSS feeds as input.
	 </Description>
	 <messageReceivers>
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only" class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver" />
		<messageReceiver  mep="http://www.w3.org/2004/08/wsdl/in-out"  class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	 </messageReceivers>
	 <parameter name="ServiceClass" locked="false">cgl.quakesim.automateddisloc.AutomatedDislocBean</parameter>
  </service>
  <service name="RunautomatedDisloc" >
	 <Description>
		Runs Disloc automatically using USGS earthquake RSS feeds as input.
	 </Description>
	 <parameter name="load-on-startup">true</parameter>
	 <messageReceivers>
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only" class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver" />
		<messageReceiver  mep="http://www.w3.org/2004/08/wsdl/in-out"  class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	 </messageReceivers>
	 <parameter name="ServiceClass" locked="false">cgl.quakesim.automateddisloc.RunautomatedDisloc</parameter>
  </service>
</serviceGroup>
